  "additionalProperties": false,
  "required": [
    "processingDate"
  ],
  "definitions": {
    "psaPspSchemeDetailsType": {
      "type": "object",
      "properties": {
        "schemeDetails": {
          "$ref": "#/definitions/schemeDetailsType"
        },
        "establisherDetails": {
          "$ref": "#/definitions/establisherDetailsType"
        },
        "trusteeDetails": {
