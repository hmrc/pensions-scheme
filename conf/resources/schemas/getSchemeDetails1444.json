    "processingDate": {
      "type": "string",
      "format": "date-time"
    },
    "formBundleNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "sapNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "psaPspSchemeDetails": {
